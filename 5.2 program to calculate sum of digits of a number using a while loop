#include<stdio.h>
int main(){
    int Number, Remainder, Sum=0;
    printf("\n Please Enter any number\n");
    scanf("%d", &Number);
    while(Number > 0)
    {
        Remainder = Number % 10;
        Sum = Sum+ Remainder;
        Number = Number / 10;
    }
    printf("\n Sum of the digits of Given Number = %d",Sum);
    return 0;
}
